		{
			"name": "create a recipe_1",
			"event": [
				{
					"listen": "test",
					"script": {
						"id": "b4e11bb6-88ce-4c14-828f-75cbf3158bd6",
						"exec": [
							"var jsonData = pm.response.json();\r",
							"var recipeId = jsonData._id;\r",
							"pm.environment.set(\"recipe_id\", recipeId);\r",
							"\r",
							"pm.test(\"Correct status code\", function() {\r",
							"    pm.response.to.have.status(201);\r",
							"});\r",
							"\r",
							"pm.test(\"Correct recipe response\", function () {\r",
							"    pm.expect(jsonData.duration).to.eql(35);\r",
							"    pm.expect(jsonData.name).to.eql(\"Homemade Baked Potato Chips\");\r",
							"});"
						],
						"type": "text/javascript"
					}
				}
			],
			"request": {
				"method": "POST",
				"header": [
					{
						"key": "Accept",
						"type": "text",
						"value": "application/json"
					},
					{
						"key": "Content-Type",
						"type": "text",
						"value": "application/json"
					}
				],
				"body": {
					"mode": "raw",
					"raw": "{\r\n  \"name\": \"Homemade Baked Potato Chips\",\r\n  \"duration\": 35,\r\n  \"instruction\": \" Preheat oven to 230 C (450 F). Line baking sheet with parchment paper. Cut the potatoes in 1/8-inch thick round slices. In a pot of boiling water cook the potato slices for 3-5 minute. Drain well and combine with oil, salt, pepper and spices (garlic, oregano, basil, etc) in a large bowl.Place potato slices on prepared baking sheet in one layer. Bake for about 15 minutes until crisp and golden brown. Gently remove the chips and place on a cooling rack or parchment paper; let them cool for few minutes and they are ready to serve.\"\r\n}\r\n"
				},
				"url": {
					"raw": "{{host}}/api/recipes",
					"host": [
						"{{host}}"
					],
					"path": [
						"api",
						"recipes"
					]
				}
			},
			"response": []
		},
